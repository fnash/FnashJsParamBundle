    "homepage": "http://github.com/fnash/FnashJsParamBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fayez Naccache",
            "email": "fayez.naccache@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "Fnash\\JsParamBundle": "" }
    },
    "target-dir": "Fnash/JsParamBundle"
}
